rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // Allow public read access to all files.
    // This is useful for images, etc. that need to be displayed in the app.
    // Restrict write access to only authenticated users.
    match /{allPaths=**} {
      allow read;
      allow write: if request.auth != null;
    }
  }
}
